<div class="row">
    <div class="col-md-12">
      <div class="panel panel-primary">
        <div class="panel-body">

          <table
            id="datatable-fixed-header"
            class="table table-striped table-bordered"
          >
            <thead>
              <tr>
                <th class="text-center">HỌ TÊN</th>
                <th class="text-center" style="width:15%">Username</th>
                <th class="text-center" style="width:15%">CMND</th>
                <th>Số dư:</th>
                <th style="width:15%">TRẠNG THÁI</th>
                <th style="width:15%"></th>
              </tr>
            </thead>

            <tbody>
              {{#each userAccount}}
                <tr>
                  <td>{{name}}</td>
                  <td>{{identity_card}}</td>
                  <td>{{user_name}}</td>
                  <td>{{active}}</td>
                  <td>  </td>
                  <td style="display:flex; justify-content: center;">
                      <button class="btn btn-primary">Notice </button>
                  </td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>