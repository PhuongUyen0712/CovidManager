<div class="container">

  <div class="row">
    <div class="col-md-12">
      <div class="panel panel-primary">
        <div class="panel-body">

          <table
            id="datatable-fixed-header"
            class="table table-striped table-bordered"
          >
            <thead>
              <tr>
                <th class="text-center">ID</th>
                <th class="text-center" style="width:15%">TYPE</th>
                <th class="text-center">DESCRIBE</th>
              </tr>
            </thead>

            <tbody>
              {{#each log}}
                <tr>
                  <td>{{id_manager}}</td>
                  <td>{{type}}</td>
                  <td>{{describe}}</td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>