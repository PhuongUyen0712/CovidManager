<div class="container">

  <div class="row">
    <div class="col-md-12">
      <div class="panel panel-primary">
        <div class="panel-body">

          <table
            id="datatable-fixed-header"
            class="table table-striped table-bordered"
          >
            <thead>
              <tr>
                <th class="text-center">Tên Gói</th>
                <th class="text-center" style="width:10%">Số Lượng</th>
                <th class="text-center" style="width:10%">Thời Gian</th>
                <th class="text-center" >Giá</th>

              </tr>
            </thead>

            <tbody>
              {{#each payment}}
              
                <tr>
                  <td> {{package_name}}</td>
                  <td>{{count}}</td>
                  <td>{{time_start}}</td>
                  <td>{{price}}</td>

                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>